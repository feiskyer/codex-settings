prefix_rule(
    pattern = ["specify", "--help"],
    decision = "allow",
    match = [["specify", "--help"]],
)

prefix_rule(
    pattern = ["specify", "check"],
    decision = "allow",
    match = [["specify", "check"]],
)

prefix_rule(
    pattern = ["specify"],
    decision = "prompt",
    match = [["specify", "init", "."]],
)
